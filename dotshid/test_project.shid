#insert scripts\zm\_shid_test3.gsh;
#insert scripts\zm\_shid_test4.gsh;

><gscprecaching><
#precache("fx", "some_cool_fx");
#precache("fx", "some_cool_fx2");
#precache("fx", "some_cool_fx3");
#precache("fx", "some_cool_fx4");

><gscpremain><
    level.shid_test_var1 = 0;
    level.shid_test_var2 = 0;
    level.shid_test_var3 = 0;

><gscpostmain><
    boat = GetEnt("boat_ent", "targetname");
    boat thread boat_think();
    level thread watch_game_over();

><gscfuncs><
function boat_think()
{
    self MoveTo(self.origin + (50, 0, 0));
    self waittill("movedone");
    level.shid_test_var1 = 1;
}

function watch_game_over()
{
    iPrintLnBold("Game Over Oh No!");
}

